    <!--
        Added the entity TestServiceClass.
    -->
    <changeSet id="20160208210109-1" author="jhipster">
        <createTable tableName="test_service_class">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_one_to_many_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="user_one_to_one_id" type="bigint">
                <constraints unique="true" nullable="true" uniqueConstraintName="ux_test_service_class_user_one_to_one_id" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
        <createTable tableName="test_service_class_user_many_to_many">
            <column name="user_many_to_manies_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="test_service_classes_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey columnNames="test_service_classes_id, user_many_to_manies_id" tableName="test_service_class_user_many_to_many"/>
        
    </changeSet>
    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here, do not remove-->
